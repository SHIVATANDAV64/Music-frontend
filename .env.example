# Frontend Environment Variables
# Copy to .env.local and fill in your values

# Jamendo Music API
VITE_JAMENDO_CLIENT_ID=your_jamendo_client_id_here

# Appwrite Configuration
VITE_APPWRITE_ENDPOINT=https://your-region.cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=your_project_id_here
VITE_DATABASE_ID=your_database_id_here

# Appwrite Function IDs (get these from Appwrite Console after deploying functions)
VITE_FUNCTION_GET_TRACKS=your_get_tracks_function_id_here
VITE_FUNCTION_GET_PODCASTS=your_get_podcasts_function_id_here
VITE_FUNCTION_MANAGE_PLAYLISTS=your_manage_playlists_function_id_here
VITE_FUNCTION_MANAGE_FAVORITES=your_manage_favorites_function_id_here
VITE_FUNCTION_RECORD_HISTORY=your_record_history_function_id_here
VITE_FUNCTION_ADMIN_UPLOAD=your_admin_upload_function_id_here
VITE_FUNCTION_SEARCH=your_search_function_id_here
VITE_FUNCTION_AUDIO_PROXY=audio-proxy
